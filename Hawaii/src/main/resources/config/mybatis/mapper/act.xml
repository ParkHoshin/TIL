<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="act"> 

	
	<!-- 목록조회 -->
	<select id="act_list" resultType="vo.ActVo">
		select * from hw_island order by hw_idx
	</select>
	<!-- Act 한개 조회 -->
	<select id="act_one" parameterType="String" resultType="vo.ActVo">
		select * from h_act where a_idx=#{ a_idx }
	</select>
	
	
	

	

</mapper>












